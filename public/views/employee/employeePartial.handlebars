<div class="col-md-4">
    <ul class="list-group">
        {{#each employees}}
        {{#unless assigned}}
        <li class="list-group-item d-flex justify-content-between align-items-center">
            {{this.id}}.
            {{this.Name}}
        </li>
        {{/unless}}
        {{/each}}
    </ul>
</div>
