<table class="table">
    <thead class="thead-light">
        <tr>
            <th scope="col">#</th>
            <th scope="col">Client Name</th>
            <th scope="col">Location</th>
            <th scope="col">Estimated Completion time</th>
        </tr>
    </thead>
    <tbody id="tbody">
        {{#each jobs}}
        {{#if dataValues.active}}
        <tr>
            <th scope="row">{{dataValues.id}}</th>
            <td>
                <p id="client_name">{{dataValues.client_name}}.</p>
            </td>
            <td id="location">{{dataValues.location}}.</td>
            <td>
                <div class="spinner-border"></div>
            </td>
        </tr>
        {{/if}}
        {{/each}}
    </tbody>
</table>
